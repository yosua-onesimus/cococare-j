	<queryString><![CDATA[SELECT m.fullName, b.date_ borrowingDate, bi.dateReturn dueDate, r.date_ returningDate, bi.borrowingCost, ri.borrowingFine
FROM lib_borrowing_items bi
JOIN lib_borrowings b ON bi.borrowing_id = b.id
LEFT JOIN lib_returning_items ri ON bi.id = ri.borrowingItem_id
LEFT JOIN lib_returnings r ON ri.returning_id = r.id
JOIN lib_members m ON b.member__id = m.id
WHERE bi.book_id = $P{book}
ORDER BY b.date_ ASC]]></queryString>

